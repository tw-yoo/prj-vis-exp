{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.0.0-rc4.json",
  "data": {
    "url": "data/temp/data/bar_ordinal_qaunt.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "size",
      "type": "ordinal",
      "sort": ["X-Small", "Small", "Medium", "Large", "X-Large"]
    },
    "y": {
      "field": "units_sold",
      "type": "quantitative"
    }
  }
}